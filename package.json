  },
  "homepage": "https://github.com/kristiania-pg6301-2022/pg6301-innlevering-SebastianHellum#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-jest": "^27.5.1",
    "concurrently": "^7.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1"
  }
}
